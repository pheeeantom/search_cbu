{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Search CBU",
    "description": "Searching in cursive, bold texts, headers or links",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css" : [
                "styles/style.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [ "images/*" ],
            "matches": [ "<all_urls>" ]
        }
    ]
}